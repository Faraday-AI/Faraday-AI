{
    "model_name": "activity_planning",
    "version": "1.0.0",
    "description": "Model for planning physical education activities",
    "input_features": [
        "class_size",
        "available_equipment",
        "time_constraints",
        "learning_objectives"
    ],
    "output_features": [
        "activity_sequence",
        "equipment_requirements",
        "time_allocation"
    ],
    "parameters": {
        "learning_rate": 0.001,
        "batch_size": 32,
        "epochs": 100
    }
} 